  "license": "ISC",
  "devDependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "sass": "^1.54.8",
    "tiny-glob": "^0.2.9"
  },
